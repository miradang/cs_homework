aliens = []
import random

for alien_number in range(30):
    new_alien = {'color': 'green', 'points': 5, 'speed': 'slow', 'random_seed': random.randint(1, 3)}
    aliens.append(new_alien)

for alien in aliens[:5]:
    if alien['color'] == 'green':
        alien['color'] = 'yellow'
        alien['speed'] = 'medium'
        alien['points'] = 10

#for alien in aliens[5:]:
#    print(aliens)
print("...")

print(f"Total Number of aliens created: {len(aliens)}")

