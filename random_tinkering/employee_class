class Employee:
    def __init__(self,first,last,salary):
        self.first = first
        self.last = last
        self.salary = int(salary)

    def give_raise(self, salary="5000"):
        self.salary += int(salary)

    def employee_full_name(self)
        fullname = f "{self.first} {selflast}"
        return fullname.title()
    
    
    

    




